{
  "name": "@ldesign/icons",
  "version": "0.1.0",
  "description": "LDesign 统一图标系统 - 2000+ SVG 图标，支持 React/Vue/Web Components，按需导入",
  "keywords": [
    "ldesign",
    "icons",
    "svg",
    "vue3",
    "react",
    "typescript",
    "web-components",
    "tree-shaking"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/icons#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ldesign/ldesign.git",
    "directory": "packages/icons"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./vue": {
      "types": "./es/vue/index.d.ts",
      "import": "./es/vue/index.js",
      "require": "./lib/vue/index.cjs"
    },
    "./vue/*": {
      "types": "./es/vue/*.d.ts",
      "import": "./es/vue/*.js",
      "require": "./lib/vue/*.cjs"
    },
    "./react": {
      "types": "./es/react/index.d.ts",
      "import": "./es/react/index.js",
      "require": "./lib/react/index.cjs"
    },
    "./react/*": {
      "types": "./es/react/*.d.ts",
      "import": "./es/react/*.js",
      "require": "./lib/react/*.cjs"
    },
    "./web-components": {
      "types": "./es/web-components/index.d.ts",
      "import": "./es/web-components/index.js",
      "require": "./lib/web-components/index.cjs"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs"
    },
    "./utils": {
      "types": "./es/utils/index.d.ts",
      "import": "./es/utils/index.js",
      "require": "./lib/utils/index.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "fonts"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,dts",
    "build:fonts": "tsx scripts/generate-fonts.ts",
    "dev": "ldesign-builder watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf es lib fonts coverage",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "generate:vue": "tsx scripts/generate-vue.ts",
    "generate:react": "tsx scripts/generate-react.ts",
    "generate:all": "pnpm run generate:vue && pnpm run generate:react && pnpm run build:fonts"
  },
  "dependencies": {
    "@ldesign/shared": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.47",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/ui": "^2.0.0",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.18.0",
    "jsdom": "^24.0.0",
    "lucide": "^0.263.1",
    "react": "^18.2.0",
    "rimraf": "^5.0.0",
    "svg2ttf": "^6.0.3",
    "svgicons2svgfont": "^15.0.1",
    "ttf2eot": "^3.1.0",
    "ttf2woff": "^3.0.0",
    "ttf2woff2": "^8.0.0",
    "typescript": "^5.7.3",
    "vitest": "^2.0.0",
    "vue": "^3.4.15"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  }
}

